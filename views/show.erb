                  <div class="flex justify-between gap-x-4 py-3">
                    <dt class="text-gray-500">25th Percentile</dt>
                    <dd class="text-gray-700">$<%= @job[:A_PCT25] %></dd>
                  </div>
                  <div class="flex justify-between gap-x-4 py-3">
                    <dt class="text-gray-500">Median</dt>
                    <dd class="text-gray-700">$<%= @job[:A_MEDIAN] %></dd>
                  </div>
                  <div class="flex justify-between gap-x-4 py-3">
                    <dt class="text-gray-500">75th Percentile</dt>
                    <dd class="text-gray-700">$<%= @job[:A_PCT75] %></dd>
                  </div>
                </dl>
            </li>
          </ul>
        </dd>
      </div>
      <div class="px-4 py-6 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-0">
        <dt class="text-sm font-medium leading-6 text-gray-900">Hourly Pay</dt>
        <dd class="mt-2 text-sm text-gray-900 sm:col-span-2 sm:mt-0">
          <ul role="list" class="divide-y divide-gray-100 rounded-md border border-gray-200">
            <li class="flex items-center justify-between py-4 pl-4 pr-5 text-sm leading-6">
                <dl class="-my-3 divide-y divide-gray-100 px-6 py-4 text-sm leading-6">
                  <div class="flex justify-between gap-x-4 py-3">
                    <dt class="text-gray-500">25th Percentile</dt>
                    <dd class="text-gray-700">$<%= @job[:H_PCT25] %></dd>
